<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.javalab.androidlab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".geoquiz.activities.QuizActivity"
            android:label="@string/title_activity_quiz"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".geoquiz.activities.LifecycleLoggerActivity"
            android:label="@string/title_activity_lifecycle_logger"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".geoquiz.activities.CheatActivity"
            android:label="@string/title_activity_cheat"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".criminalintent.activities.CrimeActivity"
            android:label="@string/title_activity_crime"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>
